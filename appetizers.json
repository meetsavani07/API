    "ingredients": [
      "2 medium beets, roasted",
      "1 can chickpeas, drained",
      "2 tbsp tahini",
      "2 tbsp lemon juice",
      "2 cloves garlic",
      "2 tbsp olive oil",
      "1/2 tsp cumin",
      "Salt to taste",
      "Goat cheese crumbles (vegan)",
      "Vegetables for dipping"
    ],
    "instructions": [
      "Wrap beets in foil and roast 45 minutes",
      "Let cool, then peel and chop",
      "Blend roasted beets with chickpeas",
      "Add tahini, lemon juice, and garlic",
      "Blend until smooth, adding water as needed",
      "Season with cumin and salt",
      "Transfer to serving bowl",
      "Drizzle with olive oil",
      "Top with vegan goat cheese crumbles",
      "Serve with fresh vegetables"
    ],
    "category": "Appetizers",
